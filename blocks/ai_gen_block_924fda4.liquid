      "default": 10
    },
    {
      "type": "range",
      "id": "mobile_icon_size",
      "min": 20,
      "max": 60,
      "step": 5,
      "unit": "px",
      "label": "Mobile icon size",
      "default": 35
    },
    {
      "type": "range",
      "id": "mobile_icon_spacing",
      "min": 5,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Mobile icon spacing",
      "default": 12
    },
    {
      "type": "range",
      "id": "mobile_heading_size",
      "min": 10,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Mobile heading size",
      "default": 13
    },
    {
      "type": "header",
      "content": "Benefit 1"
    },
    {
      "type": "text",
      "id": "heading_1",
      "label": "Heading",
      "default": "Free Shipping on Orders $15+"
    },
    {
      "type": "select",
      "id": "icon_type_1",
      "label": "Icon type",
      "options": [
        { "value": "preset", "label": "Preset icon" },
        { "value": "custom", "label": "Custom icon" }
      ],
      "default": "preset"
    },
    {
      "type": "select",
      "id": "icon_1",
      "label": "Icon",
