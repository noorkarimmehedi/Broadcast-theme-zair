{% comment %}
  Promotional Benefits Section - Modern Design
  Features: Responsive layout, custom/preset icons, modern icon library
{% endcomment %}

{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-benefits-section-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding }}px 20px;
  }

  .ai-benefits-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: {{ block.settings.column_spacing }}px;
  }

  .ai-benefit-column-{{ ai_gen_id }} {
    text-align: center;
    padding: {{ block.settings.column_padding }}px;
  }

  .ai-benefit-icon-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    margin: 0 auto {{ block.settings.icon_spacing }}px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-benefit-icon-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    stroke: {{ block.settings.icon_color }};
    fill: none;
    stroke-width: {{ block.settings.icon_stroke_width }};
    stroke-linecap: round;
    stroke-linejoin: round;
  }

  .ai-benefit-icon-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  .ai-benefit-heading-{{ ai_gen_id }} {
    font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
    font-weight: {{ block.settings.heading_font.weight }};
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0;
    line-height: 1.3;
  }

  @media screen and (max-width: 768px) {
    .ai-benefits-container-{{ ai_gen_id }} {
      grid-template-columns: repeat(3, 1fr);
      gap: {{ block.settings.mobile_spacing }}px;
    }

    .ai-benefit-column-{{ ai_gen_id }} {
      padding: {{ block.settings.mobile_padding }}px;
    }

    .ai-benefit-icon-{{ ai_gen_id }} {
      width: {{ block.settings.mobile_icon_size }}px;
      height: {{ block.settings.mobile_icon_size }}px;
      margin-bottom: {{ block.settings.mobile_icon_spacing }}px;
    }

    .ai-benefit-heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.mobile_heading_size }}px;
    }
  }
{% endstyle %}

<div class="ai-benefits-section-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-benefits-container-{{ ai_gen_id }}">
    {% for i in (1..3) %}
      {% liquid
        assign heading_key = 'heading_' | append: i
        assign icon_type_key = 'icon_type_' | append: i
        assign icon_key = 'icon_' | append: i
        assign custom_icon_key = 'custom_icon_' | append: i
        
        assign heading = block.settings[heading_key]
        assign icon_type = block.settings[icon_type_key]
        assign icon = block.settings[icon_key]
        assign custom_icon = block.settings[custom_icon_key]
      %}
      
      <div class="ai-benefit-column-{{ ai_gen_id }}">
        <div class="ai-benefit-icon-{{ ai_gen_id }}">
          {% if icon_type == 'custom' and custom_icon %}
            <img src="{{ custom_icon | image_url: width: 200 }}" alt="{{ heading | escape }}" loading="lazy">
          {% else %}
            {% case icon %}
              {% when 'shipping_fast' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
