      "default": 18
    },
    {
      "type": "header",
      "content": "Icons"
    },
    {
      "type": "range",
      "id": "icon_size",
      "min": 30,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Icon size",
      "default": 50
    },
    {
      "type": "range",
      "id": "icon_spacing",
      "min": 10,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Icon spacing",
      "default": 20
    },
    {
      "type": "range",
