        { "value": "gift_box", "label": "🎁 Gift Box" },
        { "value": "trophy", "label": "🏆 Trophy" },
        { "value": "medal", "label": "🏅 Medal" },
        { "value": "discount_tag", "label": "🏷️ Discount Tag" },
        { "value": "percentage_circle", "label": "% Percentage" },
