                  <path d="M2 8H8" stroke-width="1.5"/>
                  <path d="M2 11H6" stroke-width="1.5"/>
                </svg>
              {% when 'box_3d' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M12 2L2 7L12 12L22 7L12 2Z"/>
                  <path d="M2 17L12 22L22 17"/>
                  <path d="M2 12L12 17L22 12"/>
                </svg>
              {% when 'rocket' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M4.5 16.5C3 14 2.5 11.5 2.5 9C2.5 6.5 3 4 5.5 2.5C7 2 10 1.5 12 3.5C14 5.5 15.5 5.5 17.5 5.5C19.5 5.5 21.5 6.5 21.5 9C21.5 11.5 21 14 19.5 16.5"/>
                  <path d="M9 14L4.5 16.5L7 22L9 19L11 22L13.5 16.5"/>
                  <circle cx="12" cy="9" r="2"/>
                </svg>
              {% when 'lightning_bolt' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M13 2L3 14H12L11 22L21 10H12L13 2Z"/>
                </svg>
              {% when 'leaf' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M11 2C11 2 7 8 7 12C7 14.7614 9.23858 17 12 17C14.7614 17 17 14.7614 17 12C17 8 13 2 13 2"/>
                  <path d="M11 17C11 17 5 18 3 22"/>
                  <path d="M13 17C13 17 19 18 21 22"/>
                </svg>
              {% when 'sparkles' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M12 3L13.5 7.5L18 9L13.5 10.5L12 15L10.5 10.5L6 9L10.5 7.5L12 3Z"/>
                  <path d="M19 16L19.75 18L22 18.75L19.75 19.5L19 22L18.25 19.5L16 18.75L18.25 18L19 16Z"/>
                  <path d="M6 4L6.5 5.5L8 6L6.5 6.5L6 8L5.5 6.5L4 6L5.5 5.5L6 4Z"/>
                </svg>
              {% when 'verified_badge' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M9 12L11 14L15 10"/>
                  <path d="M21 12C21 16.9706 16.9706 21 12 21C7.02944 21 3 16.9706 3 12C3 7.02944 7.02944 3 12 3C16.9706 3 21 7.02944 21 12Z"/>
                  <path d="M12 3L13.5 1.5L15 3L16.5 1.5L18 3L19.5 4.5L21 3V12"/>
                </svg>
              {% when 'heart_hand' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M11 4C11 4 9.5 2.5 7 2.5C4.5 2.5 2.5 4.5 2.5 7C2.5 9.5 4 11 6 13L11 18L16 13C18 11 19.5 9.5 19.5 7C19.5 4.5 17.5 2.5 15 2.5C12.5 2.5 11 4 11 4Z"/>
                  <path d="M11 18V22"/>
                  <path d="M7 22H15"/>
                </svg>
              {% when 'star_shine' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M12 2L14.5 9.5L22 12L14.5 14.5L12 22L9.5 14.5L2 12L9.5 9.5L12 2Z"/>
                </svg>
              {% when 'gift_box' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="3" y="10" width="18" height="12" rx="1"/>
                  <path d="M3 10H21"/>
                  <path d="M12 10V22"/>
                  <path d="M12 10C12 10 10 8 10 6C10 4.5 11 3 12 3C13 3 14 4.5 14 6C14 8 12 10 12 10Z"/>
                  <path d="M12 10C12 10 14 8 14 6C14 4.5 13 3 12 3C11 3 10 4.5 10 6C10 8 12 10 12 10Z"/>
                </svg>
              {% when 'trophy' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M6 9H4C3.44772 9 3 8.55228 3 8V6C3 5.44772 3.44772 5 4 5H6"/>
                  <path d="M18 9H20C20.5523 9 21 8.55228 21 8V6C21 5.44772 20.5523 5 20 5H18"/>
                  <path d="M6 5H18V12C18 15.3137 15.3137 18 12 18C8.68629 18 6 15.3137 6 12V5Z"/>
                  <path d="M12 18V22"/>
                  <path d="M8 22H16"/>
                </svg>
              {% when 'shield_check' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M12 22C12 22 20 18 20 12V5L12 2L4 5V12C4 18 12 22 12 22Z"/>
                  <path d="M9 12L11 14L15 10"/>
                </svg>
              {% when 'discount_tag' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M21.41 11.58L12.41 2.58C12.05 2.22 11.55 2 11 2H4C2.9 2 2 2.9 2 4V11C2 11.55 2.22 12.05 2.59 12.42L11.59 21.42C11.95 21.78 12.45 22 13 22C13.55 22 14.05 21.78 14.41 21.41L21.41 14.41C21.78 14.05 22 13.55 22 13C22 12.45 21.77 11.94 21.41 11.58Z"/>
                  <circle cx="6.5" cy="6.5" r="1.5"/>
                  <path d="M9 15L15 9"/>
                </svg>
              {% when 'refresh_circle' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M21 12C21 16.9706 16.9706 21 12 21C7.02944 21 3 16.9706 3 12C3 7.02944 7.02944 3 12 3C16.9706 3 21 7.02944 21 12Z"/>
                  <path d="M15 9L12 12L9 9"/>
                  <path d="M12 12V16"/>
                  <path d="M8 4L12 8L16 4"/>
                </svg>
              {% when 'wallet' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="2" y="6" width="20" height="14" rx="2"/>
                  <path d="M2 10H22"/>
                  <path d="M6 6V4C6 2.89543 6.89543 2 8 2H16C17.1046 2 18 2.89543 18 4V6"/>
                  <circle cx="17" cy="15" r="1.5"/>
                </svg>
              {% when 'secure_payment' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="2" y="7" width="20" height="14" rx="2"/>
                  <path d="M2 11H22"/>
                  <path d="M12 7V3"/>
                  <path d="M9 3H15"/>
                  <circle cx="12" cy="16" r="1"/>
                </svg>
              {% when 'stopwatch' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="12" cy="13" r="8"/>
                  <path d="M12 9V13L14.5 15.5"/>
                  <path d="M10 3H14"/>
                  <path d="M12 3V5"/>
                  <path d="M18 6L19.5 4.5"/>
                </svg>
              {% when 'globe_network' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="12" cy="12" r="10"/>
                  <path d="M2 12H22"/>
                  <path d="M12 2C14.5 4.5 16 8 16 12C16 16 14.5 19.5 12 22"/>
                  <path d="M12 2C9.5 4.5 8 8 8 12C8 16 9.5 19.5 12 22"/>
                </svg>
              {% when 'megaphone' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M3 11V13C3 13.5523 3.44772 14 4 14H7L11 18V6L7 10H4C3.44772 10 3 10.4477 3 11Z"/>
                  <path d="M11 12H21"/>
                  <path d="M18 8L21 12L18 16"/>
                </svg>
              {% when 'headset' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M3 18V12C3 7.02944 7.02944 3 12 3C16.9706 3 21 7.02944 21 12V18"/>
                  <path d="M21 18C21 19.1046 20.1046 20 19 20H18C16.8954 20 16 19.1046 16 18V15C16 13.8954 16.8954 13 18 13H21V18Z"/>
                  <path d="M3 18C3 19.1046 3.89543 20 5 20H6C7.10457 20 8 19.1046 8 18V15C8 13.8954 7.10457 13 6 13H3V18Z"/>
                </svg>
              {% when 'phone_ring' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M22 16.92V19.92C22 20.4723 21.5523 20.92 21 20.92C10.5066 20.92 2 12.4134 2 1.92C2 1.36772 2.44772 0.92 3 0.92H6C6.55228 0.92 7 1.36772 7 1.92C7 4.32 7.5 6.42 8.5 8.42C8.7 8.82 8.6 9.32 8.3 9.62L6.5 11.42C8.2 14.92 11 17.72 14.5 19.42L16.3 17.62C16.6 17.32 17.1 17.22 17.5 17.42C19.5 18.42 21.6 18.92 24 18.92C24.5523 18.92 25 19.3677 25 19.92Z"/>
                  <path d="M18 3C19.5 4.5 20 6.5 20 8"/>
                  <path d="M16 5C16.5 5.5 17 6.5 17 7.5"/>
                </svg>
              {% when 'mail_open' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M4 8L12 13L20 8"/>
                  <path d="M4 8V18C4 19.1046 4.89543 20 6 20H18C19.1046 20 20 19.1046 20 18V8"/>
                  <path d="M20 8L12 2L4 8"/>
                </svg>
              {% when 'shopping_cart' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="9" cy="21" r="1"/>
                  <circle cx="20" cy="21" r="1"/>
                  <path d="M1 1H5L7.68 14.39C7.77 14.78 8.11 15 8.5 15H19.4C19.79 15 20.13 14.78 20.22 14.39L23 6H6"/>
                </svg>
              {% when 'users_group' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="9" cy="7" r="4"/>
                  <path d="M2 21V19C2 16.7909 3.79086 15 6 15H12C14.2091 15 16 16.7909 16 19V21"/>
                  <circle cx="17" cy="7" r="3"/>
                  <path d="M22 21V19.5C22 17.567 20.433 16 18.5 16H18"/>
                </svg>
              {% when 'percentage_circle' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="12" cy="12" r="10"/>
                  <path d="M15 9L9 15"/>
                  <circle cx="9.5" cy="9.5" r="1.5"/>
                  <circle cx="14.5" cy="14.5" r="1.5"/>
                </svg>
              {% when 'calendar_check' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="3" y="4" width="18" height="18" rx="2"/>
                  <path d="M16 2V6"/>
                  <path d="M8 2V6"/>
                  <path d="M3 10H21"/>
                  <path d="M9 14L11 16L15 12"/>
                </svg>
              {% when 'lock_secure' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="5" y="11" width="14" height="10" rx="2"/>
                  <path d="M12 15V17"/>
                  <circle cx="12" cy="15" r="1"/>
                  <path d="M8 11V7C8 4.79086 9.79086 3 12 3C14.2091 3 16 4.79086 16 7V11"/>
                </svg>
              {% when 'medal' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <circle cx="12" cy="12" r="6"/>
                  <path d="M15.5 7.5L18 2L22 3L18.5 8.5"/>
                  <path d="M8.5 7.5L6 2L2 3L5.5 8.5"/>
                  <circle cx="12" cy="12" r="3"/>
                </svg>
              {% when 'truck_loading' %}
                <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M14 16V8C14 6.89543 13.1046 6 12 6H4C2.89543 6 2 6.89543 2 8V16"/>
                  <circle cx="7" cy="19" r="2"/>
                  <circle cx="17" cy="19" r="2"/>
                  <path d="M14 12H16L19 15V16H22"/>
                  <path d="M14 16H9M19 16H15"/>
                  <path d="M2 12H8"/>
                </svg>
            {% endcase %}
          {% endif %}
        </div>
        
        {% if heading != blank %}
          <h3 class="ai-benefit-heading-{{ ai_gen_id }}">{{ heading }}</h3>
        {% endif %}
      </div>
    {% endfor %}
  </div>
</div>

{% schema %}
{
  "name": "Promotional benefits",
  "tag": "section",
  "settings": [
    {
      "type": "header",
      "content": "Section style"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text color",
      "default": "#1a365d"
    },
    {
      "type": "color",
      "id": "icon_color",
      "label": "Icon color",
      "default": "#1a365d"
    },
    {
      "type": "range",
      "id": "section_padding",
      "min": 20,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Section padding",
      "default": 60
